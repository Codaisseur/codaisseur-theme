{
  "name": "@codaisseur/theme",
  "version": "0.3.0",
  "private": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "dist/*.js",
    "dist/**/*.js"
  ],
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^4.1.1",
    "react": "^16.8.6"
  },
  "scripts": {
    "build": "tsc -p .",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "lint:fix": "tslint --fix -c tslint.json 'src/**/*.ts'",
    "storybook": "start-storybook",
    "test": "start-storybook --ci --smoke-test",
    "prepare": "npm run build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@storybook/addons": "^5.1.4",
    "@storybook/react": "^5.0.11",
    "@types/storybook__react": "^4.0.1",
    "babel-loader": "^8.0.6",
    "husky": "^2.3.0",
    "lint-staged": "^8.1.7",
    "prettier": "^1.17.1",
    "prop-types": "^15.7.2",
    "react-dom": "^16.8.6",
    "react-scripts": "^3.0.1",
    "storybook-addon-material-ui": "^0.9.0-alpha.19",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Codaisseur/codaisseur-theme.git"
  },
  "bugs": {
    "url": "https://github.com/Codaisseur/codaisseur-theme/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint:fix && yarn lint-staged",
      "pre-push": "yarn lint"
    }
  },
  "lint-staged": {
    "*.ts": [
      "tslint -c tslint.json"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
